- content_for :title do
	Kakitube.com | asian Episode
- content_for :meta_tags do
	= render :partial => 'application/meta_tags', :locals => { :type => "website", :title => "KakiTube | asian Episode", :url => request.url, :description => "KakiTube", :canonical => request.url, :og_title => "KakiTube | asian Episode", :og_description => "KakiTube" }

/ Page Content
.container
	.row
		.col-lg-12
			.page-header
				%h1= @asian_episode.title
	.row
		.col-md-4
			= render partial: "sidebar"
		.col-md-8
			%h2 #{@asian_season.asian.title} - #{@asian_season.title} - #{@asian_episode.title}
			%hr
			.embed-responsive.embed-responsive-16by9
				.embed-responsive-item= @asian_episode.embed.html_safe
			%hr
			= link_to 'Edit this episode', edit_asian_asian_season_asian_episode_path(@asian_season.asian, @asian_season, @asian_episode), class: "btn btn-primary"
			= link_to 'Back to Episode list', asian_asian_season_asian_episodes_path, class: "btn btn-default"
