- content_for :title do
	Kakitube.com | Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}
- content_for :meta_tags do
	= render :partial => 'application/meta_tags', :locals => { :type => "website", :title => "KakiTube | Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}", :url => request.url, :description => "KakiTube | Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}", :canonical => request.url, :og_title => "KakiTube | Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}", :og_description => "KakiTube | Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}" }

/ Page Content
.container
	.row
		.col-lg-12
			.page-header
				%h1 Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}
	.row
		.col-md-4
			= render partial: "sidebar"
		.col-md-8
			#series-list.panel.panel-default
				.panel-heading
					%h3.panel-title Add New Episode for #{@anime_season.anime.title} #{@anime_season.title}
				= render "form"
