- content_for :title do
  KakiTube | Edit Post
- content_for :meta_tags do
  = render :partial => 'application/meta_tags', :locals => { :type => "website", :title => "KakiTube | Edit Post", :url => request.url, :description => "KakiTube", :canonical => request.url, :og_title => "KakiTube | Edit Post", :og_description => "KakiTube" }
  
/ Page Content
.container
  .row
    .col-lg-12
      .page-header
        %h1
          Add New Post
          %small Subtext for header
      .panel.panel-default
        .panel-heading
          %h3.panel-title New Post Form
        = form_for @post, html: { class: "form-horizontal", method: :patch } do |f|
          = render partial: 'form', locals: {f: f, post: @post}

